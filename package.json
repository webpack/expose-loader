{
  "name": "expose-loader",
  "version": "5.0.1",
  "description": "expose loader module for webpack",
  "keywords": [
    "webpack"
  ],
  "homepage": "https://github.com/webpack/expose-loader",
  "bugs": "https://github.com/webpack/expose-loader/issues",
  "repository": "webpack/expose-loader",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/webpack"
  },
  "license": "MIT",
  "author": "Tobias Koppers @sokra",
  "main": "dist/cjs.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run build -- -w",
    "clean": "del-cli dist",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production babel src -d dist --copy-files",
    "commitlint": "commitlint --from=main",
    "security": "npm audit --production",
    "lint:prettier": "prettier --cache --list-different .",
    "lint:runtime": "es-check es5 \"dist/runtime/**/*.js\"",
    "lint:code": "eslint --cache .",
    "lint:spelling": "cspell --cache --no-must-find-files --quiet \"**/*.*\"",
    "lint": "npm-run-all -l -p \"lint:**\"",
    "fix:code": "npm run lint:code -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "fix": "npm-run-all -l fix:code fix:prettier",
    "test:only": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "npm run test:only -- --watch",
    "test:coverage": "npm run test:only -- --collectCoverageFrom=\"src/**/*.js\" --coverage",
    "pretest": "npm run lint",
    "test": "npm run test:coverage",
    "prepare": "husky && npm run build",
    "release": "standard-version"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.32.0",
    "@eslint/markdown": "^7.1.0",
    "@stylistic/eslint-plugin": "^5.2.2",
    "babel-jest": "^30.0.5",
    "babel-loader": "^10.0.0",
    "cross-env": "^7.0.3",
    "cspell": "^8.13.1",
    "del": "^7.1.0",
    "del-cli": "^5.1.0",
    "es-check": "^9.1.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-webpack": "^4.5.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.21.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-unicorn": "^60.0.0",
    "globals": "^16.3.0",
    "husky": "^9.1.4",
    "jest": "^30.0.5",
    "lint-staged": "^15.2.8",
    "memfs": "^4.11.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rx": "^4.1.0",
    "standard-version": "^9.3.0",
    "styled-components": "^6.1.12",
    "typescript-eslint": "^8.38.0",
    "webpack": "^5.93.0"
  },
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "engines": {
    "node": ">= 18.12.0"
  }
}
